public class Chapter2Problems {
	
	public static void main(String[] args) {
		int age = 5*2+16;
		System.out.println("Age is: "+age);
		
		double gpa = 3.7;
		
		char ch = 'Q';
		
		boolean b = 100 > 00;
		
		// Also byte, short, long and float.
		
		System.out.println(gpa+ch+b);
		//System.out.println(ch);
		//System.out.println(b);

	}

}
